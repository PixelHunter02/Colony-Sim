.WindowDown {
    opacity: 0;
    position: absolute;
    bottom: 0;
    width: 100%;
    height: 100%;
    translate: 0 100%;
    transition-duration: 1s;
    transition-timing-function: ease-in-out;
    display: flex;
}

.WindowUp {
    opacity: 1;
    position: absolute;
    bottom: 0;
    width: 100%;
    height: 100%;
    translate: 0 0;
    transition-duration: 1s;
    transition-timing-function: ease-in-out;
}

